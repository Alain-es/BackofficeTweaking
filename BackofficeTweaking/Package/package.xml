    </file>
    <file>
      <guid>Install.zip</guid>
      <orgPath>/App_Plugins/BackofficeTweaking</orgPath>
      <orgName>Install.zip</orgName>
    </file>
  </files>
  <info>
    <package>
      <name>BackofficeTweaking</name>
      <version>0.6.1</version>
