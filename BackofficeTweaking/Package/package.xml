      <requirements>
        <major>7</major>
        <minor>1</minor>
        <patch>4</patch>
      </requirements>
    </package>
    <author>
      <name>Alain</name>
      <website>https://github.com/Alain-es/BackofficeTweaking</website>
    </author>
    <readme><![CDATA[BackofficeTweaking requires Umbraco v7.1.4 or above]]></readme>
